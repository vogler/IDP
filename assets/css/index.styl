html, body
  height 100%
  margin 0
  padding 0
  font-family Arial, Sans

[onclick]
  cursor pointer

// button
//   cursor default!important

#layoutMenu
  position absolute
  z-index 1

#map_canvas
  height 80%

#controls
  // padding .8em
  // box-sizing border-box
  width 250px
  height 80%
  float right
#speed_slider
  margin-top 3px
#stroke_spinner
  width 2em
  border none
  padding 0
  margin-bottom 0


@media print
  html, body
    height auto
  
  #map_canvas
    height 650px